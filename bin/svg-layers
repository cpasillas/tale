#!/usr/bin/env python

from cixar.python.xml.tags import tags, Tag
from sys import stdin, stdout, argv

args = argv[1:]
if args:
    f = file(args[0])
else:
    f = stdin

image = Tag.parse(f)
for child in image:
    if (
        isinstance(child, Tag) and
        child.name == 'g' and
        'inkscape:label' in child
    ):
        print child['inkscape:label']

